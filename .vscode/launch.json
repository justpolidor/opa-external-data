    "version": "0.2.0",
    "configurations": [
        {
            "name": "Application Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {"TLS_KEY_FILE":"certs/server.key", "TLS_CERT_FILE":"certs/server.crt", "COSIGN_PUBLIC_KEY_PATH":"/Users/justinpolidori/.cosign/cosign.pub"},

            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 1024,
                "maxArrayValues": 64,
                "maxStructFields": -1
            },

            "trace": "log"
//            "buildFlags": "-tags dev -ldflags '-X main.BuildDate=2021-04-28T19:38:16+03:00'"
        }
    ]
